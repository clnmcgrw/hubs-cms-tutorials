---
import { getCollection } from 'astro:content';
import Layout from '@/layouts/Layout.astro';

export async function getStaticPaths() {
  const allPosts = await getCollection('difficulty');
  return allPosts.map(item => ({
    params: { difficulty: item.data.name.toLowerCase() }
  }));
};
---

<Layout promoBar bodyClass="tips-by-difficulty">
  
</Layout>
